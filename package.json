{
  "name": "pd3-vault-cracker",
  "private": true,
  "version": "0.1.3",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "dev:remote": "vite --host --open",
    "build": "pwa-assets-generator && vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tscon fig.json",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --open",
    "test:watch": "vitest",
    "lint": "eslint -c .eslintrc --ext .svelte,.ts src/",
    "lint:styles": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\"",
    "format": "eslint -c .eslintrc --fix --ext=.svelte,.ts --fix src",
    "format:prettier": "prettier --write src/**/{*.svelte,*.ts,*.scss}",
    "format:stylelint": "stylelint --config .stylelintrc \"src/**/*.{css,scss}\" --fix",
    "prepare": "husky install",
    "generate-pwa-assets": "pwa-assets-generator",
    "serve-json": "json-server --watch db.json --port 4000"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.3",
    "@picocss/pico": "^1.5.11",
    "@rollup/plugin-alias": "^5.0.1",
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^1.26.0",
    "@sveltejs/package": "^2.2.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@testing-library/jest-dom": "^6.3.0",
    "@testing-library/svelte": "^4.1.0",
    "@tsconfig/svelte": "^5.0.2",
    "@types/crypto-js": "^4.2.1",
    "@types/three": "^0.158.3",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@unocss/core": "latest",
    "@unocss/eslint-config": "^0.58.4",
    "@unocss/extractor-pug": "^0.58.4",
    "@unocss/preset-icons": "latest",
    "@unocss/preset-uno": "^0.58.4",
    "@unocss/svelte-scoped": "^0.58.4",
    "@unocss/transformer-directives": "^0.58.4",
    "@vite-pwa/assets-generator": "^0.2.3",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.34.0",
    "husky": "^9.0.6",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.2.4",
    "prettier-plugin-css-order": "^2.0.1",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.70.0",
    "sass-loader": "^14.0.0",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^16.2.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-prettier": "^5.0.0",
    "svelte": "^4.2.9",
    "svelte-check": "^3.6.3",
    "svelte-i18n": "^4.0.0",
    "svelte-preprocess": "^5.0.4",
    "sveltekit-autoimport": "^1.7.1",
    "tslib": "^2.6.2",
    "typed.js": "^2.0.12",
    "typescript": "^5.3.3",
    "unocss": "^0.58.4",
    "vite": "^5.0.12",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-pages": "^0.32.0",
    "vite-plugin-progress": "^0.0.7",
    "vite-plugin-pwa": "^0.17.5",
    "vite-plugin-restart": "^0.4.0",
    "vite-plugin-svg-icons": "^2.0.1"
  },
  "dependencies": {
    "@splinetool/runtime": "^0.9.508",
    "@sveltejs/kit": "^1.26.0",
    "@sxzz/eslint-config": "^3.7.0",
    "@types/cookie": "^0.5.3",
    "@unocss/extractor-svelte": "^0.57.7",
    "cookie": "^0.5.0",
    "crypto-js": "^4.2.0",
    "gsap": "3.8.0",
    "node-sass": "^9.0.0",
    "svelte-persisted-store": "^0.8.0",
    "svelte-spa-router": "^3.3.0",
    "three": "^0.158.0",
    "vanilla-tilt": "^1.8.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
